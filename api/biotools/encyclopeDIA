acquisition-based spectrum libraries alone.","date":"2018-12-01T00:00:00Z","citationCount":88,"authors":[{"name":"Searle B.C."},{"name":"Pino L.K."},{"name":"Egertson J.D."},{"name":"Ting Y.S."},{"name":"Lawrence R.T."},{"name":"MacLean B.X."},{"name":"Villen J."},{"name":"MacCoss M.J."}],"journal":"Nature Communications"}}],"credit":[{"name":"Brian Searle","email":"bsearle@systemsbiology.org","url":null,"orcidid":null,"gridid":null,"rorid":null,"fundrefid":null,"typeEntity":null,"typeRole":["Primary contact"],"note":null}],"community":null,"owner":"vtsiamis","additionDate":"2019-06-18T12:42:38Z","lastUpdate":"2020-06-16T10:55:23Z","editPermission":{"type":"private","authors":[]},"validated":1,"homepage_status":0,"elixir_badge":0,"confidence_flag":null}